#include <iostream>

using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    int k, result; 
    cin>>k;
    
    if(k==1)
        result = 1;
    else if((k==2)||(k==3))
        result = 3;
    else if((k==4)||(k==5))
        result = 5;
    else if((k>5)&&(k<11))
        result = 10;
    else if((k>10)&&(k<26))
        result = 25;
    else if((k>25)&&(k<51))
        result = 50;
    else
        result = 100;

    std::cout<<"Top "<<result<<'\n';

    return 0;
}
